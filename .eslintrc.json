{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": "error",
    "no-undef": "error",
    "quotes": "off",
    "semi": "off",
    "comma-dangle": "off",
    "space-before-function-paren": "off",
    "camelcase": "off"
  }
}
